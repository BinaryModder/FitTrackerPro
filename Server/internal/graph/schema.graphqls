# GraphQL schema example
#
# https://gqlgen.com/getting-started/


# Здесь распологается User
type User{
  id: ID!
  username: String!
  email: String
  first_name: String
  last_name: String
  password: String
}
input NewUser{
  username: String!
  email: String
  first_name: String
  last_name: String
  password: String
}

type Mutation{
  createUser(input: NewUser!): User!
}

type Query{
  users:[User!]!
}
